    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Random Image",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "-r",
            ]
        },
        {
            "name": "Python Debugger: Random Image from List",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "-rn",
                "taillow,swellow,berry",
            ]
        },
        {
            "name": "Python Debugger: Image by Name",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "-n",
                "taillow",
            ]
        }
    ]
}